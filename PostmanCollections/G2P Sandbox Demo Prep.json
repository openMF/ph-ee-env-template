						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"function randomString(minValue, maxValue, dataSet = 'abcdefghijklmnopqrstuvwxyz0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ') {",
									"    if (!minValue) {",
									"        minValue = 1;",
									"        maxValue = 4;",
									"    }",
									"",
									"    if (!maxValue) {",
									"        maxValue = minValue;",
									"    }",
									"",
									"    let length = _.random(minValue, maxValue),",
									"        randomString = \"\";",
									"",
									"    for (let i = 0; i < length; i++)",
									"        randomString += dataSet.charAt(Math.floor(Math.random() * dataSet.length));",
									"    return randomString;",
									"}",
									"",
									"pm.variables.replaceIn('{{$randomFirstName}}');",
									"pm.environment.set('lastname', randomString());",
									"",
									"const monthNames = [\"January\",\"February\",\"March\",\"April\",\"May\",\"June\",\"July\",\"August\",\"September\",\"October\",\"November\",\"December\",]",
									"today = new Date();",
									"day = today.getDate();",
									"month = monthNames[new Date().getMonth()];",
									"year = today.getFullYear();",
									"date = day + ' ' + month + ' ' + year",
									"pm.environment.set(\"activationDate\", date);",
									"pm.environment.set(\"submissionDate\", date);",
									""
								],
								"type": "text/javascript"
							}
						}
